							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "pl_point_admin_process",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"country": {
										"value": "@item()",
										"type": "Expression"
									}
								}
							}
						}
					]
				}
			}
		],
		"folder": {
			"name": "AdminProcessPipeline"
		},
